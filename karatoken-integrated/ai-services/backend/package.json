{
  "name": "karatoken-backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "build": "tsc",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.js --runInBand --detectOpenHandles --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config=jest.config.js",
    "test:ci": "cross-env NODE_ENV=test jest --ci --coverage --config=jest.config.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "postinstall": "npm run prepare"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.17.0",
    "@tensorflow/tfjs-node": "^4.17.0",
    "@tensorflow-models/speech-commands": "^0.5.4",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow-models/toxicity": "^1.2.2",
    "@tensorflow-models/posenet": "^2.2.2",
    "@tensorflow-models/body-pix": "^2.2.0",
    "@tensorflow-models/facemesh": "0.0.3",
    "@tensorflow-models/handpose": "0.0.7",
    "@tensorflow-models/pose-detection": "^1.0.0",
    "@tensorflow-models/face-landmarks-detection": "^1.0.1",
    "@tensorflow-models/face-detection": "^1.0.2",
    "@tensorflow-models/body-segmentation": "^1.0.0",
    "audio-buffer": "^6.0.1",
    "audio-context": "^1.0.1",
    "audio-lena": "^1.0.3",
    "audio-loader": "^1.0.3",
    "audio-pcm-format": "^1.0.2",
    "audio-speaker": "^0.3.0",
    "audio-stream": "^1.0.2",
    "audio-through": "^1.0.1",
    "audio-type": "^1.0.0",
    "audio-worklet": "^0.2.2",
    "audiobuffer-to-wav": "^1.0.2",
    "audiobuffer-utils": "^4.0.1",
    "audiobuffer-to-wav-arraybuffer": "^1.0.1",
    "@distube/ytdl-core": "^4.16.12",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@types/socket.io": "^3.0.1",
    "@types/uuid": "^10.0.0",
    "@types/web-audio-api": "^0.0.34",
    "@vitalets/google-translate-api": "^9.2.0",
    "axios": "^1.11.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fluent-ffmpeg": "^2.1.3",
    "morgan": "^1.10.0",
    "pitchy": "^4.0.0",
    "replicate": "^1.0.1",
    "socket.io": "^4.8.1",
    "transformers": "^2.12.0",
    "uuid": "^11.1.0",
    "web-audio-api": "^0.9.2",
    "whisper-node": "^1.0.0",
    "ws": "^8.17.1",
    "youtube-dl-exec": "^2.8.2",
    "ytdl-core": "^4.11.5",
    "ytsr": "^3.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.28",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/build/**",
      "!**/public/**"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  }
}
